<section class="_container">
  <div class="panel">
    <ul id="user-list-online" class="list-group mx-1">
      <% @users.each do |user| %> 
        <li class="user_<%= user.id %> users list-group-item">
          <%= user.email %>  
        </li>    
      <% end %>
    </ul>
  </div>
  <div class="hero">
    <div class="square cat cat-a">
      
    </div>
    <div class="square cat cat-b">
      
    </div>
    <div class="square cat cat-c">
      
    </div>
    <div class="cat cat-d">
      
    </div>
    <div class="cat cat-e">
      
    </div>
    <div class="circle cat cat-f">
      
    </div>
    <div class="cat cat-g">
      
    </div>
    <div class="circle cat cat-h">
      
    </div>
    <div class="circle  cat cat-i">
      
    </div>
  </div>
  <div class="footer">    
  </div>
</section>
<style>
  <% group = current_user.groups.last %> 
  .circle {
    background-image: url( <%= url_for(group.icons.first.image) %> );
    background-color: <%= group.icons.first.color %>;
  }
  .square {
    background-image: url( <%= url_for(group.icons.last.image) %> );
    background-color: <%= group.icons.last.color %>;
  }
</style>